[
    {{#each this}}
    {
        "name": "dev-event-{{moduleName}}-{{eventName}}-{{roleName}}-worker",
        "script": "{{filePath}}",
        "watch": true,
        "log_date_format": "YYYY-MM-DD HH:mm:ss",
        "max_memory_restart" : "200M",
        "env": {
            "NODE_ENV": "development",
            "DEBUG": "cqrs*",
            "DEBUG_COLORS": true
        }
    },
    {{/each}}
]