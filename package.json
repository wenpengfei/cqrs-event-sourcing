{
  "name": "cqrs-event-sourcing",
  "version": "1.0.0",
  "description": "cqrs-event-sourcing",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "cleanTemplate": "node internal/clean.js",
    "commandHandlers:dev": "pm2 startOrReload pm2/command.development.json --update-env",
    "commandHandlers:prod": "pm2 startOrReload pm2/command.production.json --update-env",
    "eventHandlers:dev": "pm2 startOrReload pm2/event.development.json --update-env",
    "eventHandlers:prod": "pm2 startOrReload pm2/event.production.json --update-env",
    "api:dev": "pm2 startOrReload pm2/api.development.json --update-env",
    "api:prod": "pm2 startOrReload pm2/api.production.json --update-env",
    "generate:pm2conf": "node internal/pm2conf.js",
    "generate": "node internal/generate.js",
    "generate:apidoc": "node_modules/.bin/apidoc -i src/domain/ -o apidoc/",
    "syncTable": "node dist/persistents/mysql/adapter/syncTable",
    "dev": "npm run generate:pm2conf && npm run commandHandlers:dev && npm run eventHandlers:dev && npm run api:dev",
    "prod": "npm run compile && npm run generate:pm2conf && npm run commandHandlers:prod && npm run eventHandlers:prod && npm run api:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "cqrs-lite": "^1.0.21",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "mongoose": "^5.2.3",
    "mysql2": "^1.5.3",
    "rabbitmq-broker": "^1.0.0",
    "ramda": "^0.25.0",
    "sequelize": "^4.38.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/ramda": "^0.25.45",
    "@types/sequelize": "^4.27.33",
    "apidoc": "^0.17.7",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "debug": "^3.1.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "handlebars-helpers": "^0.10.0",
    "heapdump": "^0.3.12",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  }
}
